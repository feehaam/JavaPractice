type Query {
    getAccommodationById(accommodationId: Int!): Accommodation
    getAccommodationsByType(type: String!): [Accommodation]

    getTourSpotById(tourSpotId: Int!): TourSpot
    getTourSpotsByRating(minRating: Int, maxRating: Int): [TourSpot]

    getTouristById(touristId: Int!): Tourist
    getTouristByName(name: String!): [Tourist]

    getTestById(id: Int!): TestModel
    getTests: [TestModel]
}